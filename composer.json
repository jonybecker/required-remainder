{
    "name": "jony/required-remainder",
    "description": "Required Remainder",
    "type": "project",
    "authors": [
        {
            "name": "jonybecker",
            "email": "jony.becker91@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.17",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "RequiredRemainder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "calc": "php index.php",
        "test": "phpunit tests",
        "test:coverage": "phpunit tests --coverage-html coverage",
        "test:unit": "phpunit tests/unit",
        "test:e2e": "phpunit tests/e2e",
        "cs-fix": "php-cs-fixer fix ."
    }
}
